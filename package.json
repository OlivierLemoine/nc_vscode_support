{
    "name": "nsc",
    "displayName": "nsc",
    "description": "nsc",
    "version": "0.0.2",
    "publisher": "OlivierLemoine",
    "engines": {
        "vscode": "^1.33.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onLanguage:nsc"
    ],
    "main": "./extension.js",
    "contributes": {
        "languages": [
            {
                "id": "nsc",
                "aliases": [
                    "nsc",
                    "nsc"
                ],
                "extensions": [
                    "nsc"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "nsc",
                "scopeName": "source.nsc",
                "path": "./syntaxes/nsc.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "nsc",
                "path": "./snippets.json"
            }
        ]
    },
    "devDependencies": {
        "typescript": "^3.3.1",
        "vscode": "^1.1.28",
        "eslint": "^5.13.0",
        "@types/node": "^10.12.21",
        "@types/mocha": "^2.2.42"
    }
}
